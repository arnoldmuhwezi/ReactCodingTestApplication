@model IEnumerable<CodingTestApplication.Models.TransactionTypes>

@{
    ViewBag.Title = "Index";
}

<h2>TransactionType List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TransactionName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.TransactionTypeID }) |
                @Html.ActionLink("Details", "Details", new { id = item.TransactionTypeID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.TransactionTypeID })
            </td>
        </tr>
    }

</table>

<p><a href="/Home/">Home</a></p>
